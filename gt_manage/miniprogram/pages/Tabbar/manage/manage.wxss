/* miniprogram/pages/Tabbar/manmage/manage.wxss */

.manage{
  width: 94%;
  padding: 10px 3%;
  /* background-color: #fff; */
}
.manage-content{
  margin-top: 30px;
  display: flex;
  align-items: center;
  /* justify-content: space-around; */
  flex-wrap: wrap;
  width: 100%;
}
.item{
  width: calc(100% / 3);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  margin: 10px 0;
}
.item > image{
  width: 30px;
  height: 30px;
  margin-bottom: 5px;
}